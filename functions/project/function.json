{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": ["get", "post"]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    },
    {
      "name": "projectDocument",
      "type": "cosmosDB",
      "databaseName": "logs-dev",
      "collectionName": "projects",
      "createIfNotExists": true,
      "connectionStringSetting": "geqldb_DOCUMENTDB",
      "direction": "out"
    }
  ],
  "scriptFile": "../dist/project/index.js"
}
